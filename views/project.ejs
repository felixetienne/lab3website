<article class="mod-article">
  <% include included/standardContent.ejs %>
  <% if(project.images.length || project.medias.length){%>
    <div class="mart--medias">
      <%
      for(var i=0; i<project.images.length; i++){
        var image = project.images[i]; %>
        <div class="mart--media">
          <img src="<%- image.path %>" alt="<%- image.title %>" />
        </div>
      <%}%>
      <%
      for(var i=0; i<project.medias.length; i++){
        var media = project.medias[i]; %>
        <div class="mart--media">
          <h2><%- media.typeTitle %></h2>
          <%- media.content %>
        </div>
      <%}%>
    </div>
  <%}%>
</article>
